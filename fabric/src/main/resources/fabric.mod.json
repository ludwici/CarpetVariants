{
  "schemaVersion": 1,
  "id": "carpetvariants",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_authors}"
  ],
  "contact": {
    "homepage": "${mod_url}",
    "sources": "${mod_url}"
  },
  "license": "${mod_license}",
  "icon": "logo.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.ludwici.carpetvariants.CarpetVariantsModFabric"
    ],
    "client": [
      "com.ludwici.carpetvariants.client.CarpetVariantsModFabricClient"
    ]
  },
  "mixins": [
    "${mod_id}.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "minecraft": ">=${minecraft_version}",
    "java": ">=${java_version}",
    "fabric-api": ">=${fabric_version}"
  }
}
